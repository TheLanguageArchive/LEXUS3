  <xs:element name="ConstraintSet">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="feat"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="Constraint"/>
